#include <iostream>
#include <string>

using namespace std;

int main() {
    string input;
    getline(cin, input);
    // Проверка на длину строки
    if (input.length() >= 4) {
        // вывод 2-4 символа подстроки
        cout << "Подстрока: " << input.substr(1, 3) << endl;
    // нахождение первого a в строке
    int index = input.find("a");
    cout << index << endl;
}
